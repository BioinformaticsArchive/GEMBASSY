application: gamino_info [
  documentation: "Prints out basic amino acid sequence statistics"
  groups: "Protein:Composition"
  embassy: "gembassy"
]

section: input [
  information: "Input section"
  type: "page"
]
  seqall: sequence [
    parameter: "Y"
    type: "Protein"
  ]
endsection: input

section: output [
  information: "Output section"
  type: "page"
]

  outfile: outfile [
    parameter: "Y"
  ]

endsection: output
